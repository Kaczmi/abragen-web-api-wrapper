{
    "name": "kaczmi/abragen-web-api-wrapper",
    "description": "Intuitive fluent interface for AbraGEN WebAPI",
    "type": "library",
    "require": {
    	"php": ">= 7.4",
        "nette/di": "^2.4",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "authors": [
        {
            "name": "Lukáš Křehula",
            "email": "lukas@krehula.net"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "nette/tester": ">= 1.7.2",
        "nette/robot-loader": "^3.1",
        "phpstan/phpstan": "^0.12.43",
        "slevomat/coding-standard": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "AbraApi\\": "src/AbraApi",
            "AbraApi\\DI\\": "src/DI"
        }
    }
}
